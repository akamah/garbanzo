%{
grammar.rules[:whitespace] = ["\n", " "].choice;
grammar.rules[:sentence].children << Call.new(:whitespace);
grammar.rules[:number] = lambda {|s|
  if md = s.match(/^(\d)+/)
    num = Garbanzo::Num.new(md[1].to_i)
    return num, s[s.end(0)..-1]
  else
    raise ParseError, "expected number"
  end
}
%}
3